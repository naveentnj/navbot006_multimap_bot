<launch>

<rosparam file="$(find navbot006_description)/launch/controller.yaml" command="load"/>
<node args="left_wheel_joint_position_controller right_wheel_joint_position_controller joint_state_controller " name="controller_spawner" ns="navbot006" output="screen" pkg="controller_manager" respawn="false" type="spawner"/>
<node name="robot_state_publisher" output="screen" pkg="robot_state_publisher" respawn="false" type="robot_state_publisher">
  <remap from="/joint_states" to="/navbot006/joint_states"/>
</node>

</launch>